{
   "semi": true,
   "trailingComma": "es5",
   "singleQuote": true,
   "tabWidth": 3,
   "useTabs": false,
   "printWidth": 100,
   "arrowParens": "avoid",
   "endOfLine": "lf",
   "plugins": ["prettier-plugin-astro"],
   "overrides": [
      {
         "files": "*.astro",
         "options": {
            "parser": "astro"
         }
      }
   ]
}
